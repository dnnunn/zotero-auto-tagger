{
  "manifest_version": 2,
  "name": "Zotero Auto-Tagger",
  "version": "0.2.0",
  "description": "Automatically tag Zotero items using PubMed and Claude AI",
  "homepage_url": "https://github.com/dnnunn/zotero-auto-tagger",
  "author": "David Nunn",
  "icons": {
    "16": "chrome/content/icons/icon-16.png",
    "32": "chrome/content/icons/icon-32.png",
    "48": "chrome/content/icons/icon-48.png",
    "96": "chrome/content/icons/icon-96.png"
  },
  "applications": {
    "zotero": {
      "id": "zotero-auto-tagger@dnnunn.github.io",
      "strict_min_version": "7.0",
      "strict_max_version": "7.*",
      "update_url": "https://raw.githubusercontent.com/dnnunn/zotero-auto-tagger/main/update.json"
    }
  }
}
