{
  "manifest_version": 2,
  "name": "Zotero Auto-Tagger",
  "version": "0.1.0",
  "description": "Automatically tag Zotero items with keywords from PubMed",
  "homepage_url": "https://github.com/dnnunn/zotero-auto-tagger",
  "author": "David Nunn",
  "applications": {
    "zotero": {
      "id": "zotero-auto-tagger@dnnunn.github.io",
      "update_url": "https://raw.githubusercontent.com/dnnunn/zotero-auto-tagger/main/update.json",
      "strict_min_version": "6.999",
      "strict_max_version": "7.0.*"
    }
  },
  "icons": {
    "48": "chrome/content/icons/icon@48px.png",
    "96": "chrome/content/icons/icon@96px.png"
  }
}
